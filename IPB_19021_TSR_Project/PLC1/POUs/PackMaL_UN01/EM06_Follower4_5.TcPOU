<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.6">
  <POU Name="EM06_Follower4_5" Id="{cce7950b-8550-4824-b8d4-f61665156248}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM EM06_Follower4_5
VAR
	ref_Master: REFERENCE TO UDT_AXISADMIN;
	ref_Follower1: REFERENCE TO UDT_AXISADMIN;
	ref_Follower2: REFERENCE TO UDT_AXISADMIN;
	Ref_MachineStateModel: REFERENCE TO PackMlv3_StateModel;
	Ref_Unit: REFERENCE TO UDT_UnitAdmin;	
	Loc_Resetting_Seq: INT;//
	EM_No: INT:=6;
	tonAbortingWatchDog: TON;

	
	MC_GearIn_Follower1: MC_GearIn;
	MC_CamIn_Follower1: MC_CamIn_V2;
	MC_CamTableSelect_Follower1: MC_CamTableSelect;
	CamTableFollower1: MC_CAM_REF;
	MC_GearIn_Follower2: MC_GearIn;
	MC_CamIn_Follower2: MC_CamIn_v2;
	MC_CamTableSelect_Follower2: MC_CamTableSelect;
	CamTableFollower2: MC_CAM_REF;
	
    MC_ReadCamTableSlaveDynamicsFollwer1: MC_ReadCamTableSlaveDynamics;
    MC_ReadCamTableSlaveDynamicsFollwer2: MC_ReadCamTableSlaveDynamics;
	
	table1: ARRAY[1..255] OF MC_MotionFunctionPoint; (*用于定义电子凸轮表中的关键点*)
	table2: ARRAY[1..255] OF MC_MotionFunctionPoint; (*用于定义电子凸轮表中的关键点*)

	lrFollower2PosLast: LREAL;
	diPos1Mod: DINT;
	diPos2Mod: DINT;
	rtInDropPos: r_trig;


	iInDropPosCnt: INT;
	tonFollowerSeqDelay: Ton;
	lrFollow1CamDynPos: LREAL;
	lrFollow2CamDynPos: LREAL;
	lrFollow2LatchPos: LREAL;
	lrFollow1LatchPos: LREAL;


    tonFollowerSeq: TON;
	FollowerSeq: SequenceTransfer;	
	lrPosCnt: INT;
	i: INT;
	bRecipeComplete: BOOL;
	rtMasterInRange: R_TRIG;
	bTriggerPos: BOOL;
	tonDelayMotion: TON;
END_VAR

VAR CONSTANT
	iCamID1: MC_CAM_ID:=4;	
    iCamID2: MC_CAM_ID:=5;		
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[//减速比10/1 同步轮周长400 ，皮带总长3300，7个工位，工位间距60
Ref_MachineStateModel REF= UN01_MachineStateModel;
