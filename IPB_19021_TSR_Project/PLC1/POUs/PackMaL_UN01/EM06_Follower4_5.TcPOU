    <Action Name="CM03_Follower" Id="{c063a184-c3e2-4600-a657-70dad5cce693}">
      <Implementation>
        <ST><![CDATA[CASE FollowerSeq.Now OF
	0: 
	   IF Ref_MachineStateModel.Sts_Resetting THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= Seq_Resetting);  
	   END_IF	
	   IF Ref_MachineStateModel.Sts_Stopping  THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);  
	   END_IF
	   IF Ref_MachineStateModel.Sts_Execute  THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= 110);   
	   END_IF
{region Resetting} 
	10:
		IF NOT ref_Follower1.o.PowerOn  THEN;
		   ref_Follower1.i.Enable:=TRUE;
		END_IF
		IF NOT ref_Follower2.o.PowerOn  THEN;
		   ref_Follower2.i.Enable:=TRUE;
		END_IF	
		
		IF ref_Follower1.o.PowerOn AND ref_Follower2.o.PowerOn THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 20);  
		END_IF
        IF Ref_MachineStateModel.Sts_Stopping  THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);  
	    END_IF	
		
	20:
		IF glb_AllAxisHomed_UN01 THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 21);
		   ELSE 
		   FollowerSeq(ExeCute:= TRUE, Next:= 30);
		END_IF
		
	21:
		IF ref_Master.o.Homed AND ref_Master.o.PowerOn THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 60);//轴组耦合
		END_IF
	30:
	   glb_lrMasterWrkCnt:=0;
	   glb_lrMasterTargetCnt:=0;
	   bRecipeComplete:=FALSE;
       MEMSET(destAddr:= ADR(glb_BinBelt4_5), fillByte:= 0, n:= SIZEOF(glb_BinBelt4_5));      
	   FollowerSeq(ExeCute:= TRUE, Next:= 40);
	
	40: 
	   ref_Follower1.i.GearIn:=TRUE;
	   IF ref_Follower1.o.InGear THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= 41);
	   END_IF
	   
	41:
       ref_Follower2.i.HomeMode:=0;
       ref_Follower2.i.HomeOffset:=290;  //	
	   tonFollowerSeq(IN:=TRUE , PT:=T#1S , Q=> , ET=> );
	   IF tonFollowerSeq.Q THEN
		  tonFollowerSeq(IN:=FALSE , PT:=T#1S , Q=> , ET=> );	   
			ref_Follower2.i.Home:=TRUE;
			FollowerSeq(ExeCute:= TRUE, Next:= 42);
	   END_IF
	   
	42:
       IF ref_Follower2.o.HomeDone THEN
		  ref_Follower2.i.Home:=FALSE;
		  FollowerSeq(ExeCute:= TRUE, Next:= 43); 
	   ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
		       ref_Follower2.i.Home:=FALSE;
               FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);
	        END_IF
	   END_IF
	   
	43:
	   ref_Follower1.i.GearOut:=TRUE;
	   IF ref_Follower1.o.GearOutDone AND NOT ref_Follower1.o.InSync THEN
		  ref_Follower1.i.GearOut:=FALSE;
		  ref_Follower1.i.GearIn:=FALSE;
		  FollowerSeq(ExeCute:= TRUE, Next:= 44); 
	   END_IF
	   
	44:
	   IF ref_Follower1.o.StandStill THEN
          FollowerSeq(ExeCute:= TRUE, Next:= 45);
	   ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
		       FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);   
		    END_IF	  
	   END_IF
	   
	45:
	   ref_Follower2.i.GearIn:=TRUE;
	   IF ref_Follower2.o.InGear THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= 46); 
	   END_IF  
	46:
	      ref_Follower1.i.HomeOffset:=290; // 
	      tonFollowerSeq(IN:=TRUE , PT:=T#1S , Q=> , ET=> );
	   IF tonFollowerSeq.Q THEN
			tonFollowerSeq(IN:=FALSE , PT:=T#1S , Q=> , ET=> );	   
			ref_Follower1.i.Home:=TRUE;
			FollowerSeq(ExeCute:= TRUE, Next:= 47); 
	   END_IF
	   
	47:
       IF ref_Follower1.o.HomeDone THEN
		   ref_Follower1.i.Home:=FALSE;
		   FollowerSeq(ExeCute:= TRUE, Next:= 48);
	   ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
		       ref_Follower1.i.Home:=FALSE;
               FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);   
		    END_IF 
	   END_IF
	48:
    	 ref_Follower1.i.Pos:=0 ; 
	     ref_Follower1.i.Vel:=100;
	     ref_Follower1.i.Acc:=600;
	     ref_Follower1.i.Dec:=600;   
		 FollowerSeq(ExeCute:= TRUE, Next:= 49);
    49: 
		 ref_Follower1.i.MoveAbs:=TRUE;
	     IF  ref_Follower1.o.CmdDone  THEN
			 ref_Follower1.i.MoveAbs:=FALSE; 
			 FollowerSeq(ExeCute:= TRUE, Next:= 50);
	     ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
		         ref_Follower1.i.MoveAbs:=FALSE;
                 FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);   
		      END_IF			 
		 END_IF
	      
	50:
	   ref_Follower2.i.GearOut:=TRUE;
	   IF ref_Follower2.o.GearOutDone  THEN
		  ref_Follower2.i.GearOut:=FALSE;
		  ref_Follower2.i.GearIn:=FALSE;
		  FollowerSeq(ExeCute:= TRUE, Next:= 51); 
	   END_IF  
	51: 
        tonFollowerSeq(IN:=TRUE , PT:=T#500MS , Q=> , ET=> );
		IF  ref_Follower2.o.StandStill AND tonFollowerSeq.Q  THEN
			 tonFollowerSeq(IN:=FALSE , PT:=T#500MS , Q=> , ET=> );
		     FollowerSeq(ExeCute:= TRUE, Next:= 52);
	    END_IF
		
	52: 
		 ref_Follower2.i.Dist:=440-ref_Follower2.o.ActPos ; 
         lrFollower2PosLast:=ref_Follower2.i.Pos;
	     ref_Follower2.i.Vel:=100;
	     ref_Follower2.i.Acc:=6000;
	     ref_Follower2.i.Dec:=600;
		 FollowerSeq(ExeCute:= TRUE, Next:= 53);
		 
    53:  ref_Follower2.i.MoveReal:=TRUE; 
		 FollowerSeq(ExeCute:= TRUE, Next:= 54);

    54:		 
         IF  ref_Follower2.o.CmdDone  THEN
			 ref_Follower2.i.MoveReal:=FALSE; 
			 FollowerSeq(ExeCute:= TRUE, Next:= 55);
	     ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
                 FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);    
		      END_IF			 
		 END_IF
    55: 		 
	   ref_Follower2.i.Home:=TRUE;
	   ref_Follower2.i.HomeOffset:=0;  
	   ref_Follower2.i.HomeMode:=2;
	   FollowerSeq(ExeCute:= TRUE, Next:= 56); 
	   
	56:
       IF ref_Follower2.o.HomeDone THEN
		   ref_Follower2.i.Home:=FALSE;
		   FollowerSeq(ExeCute:= TRUE, Next:= 60);  
	   ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
		       FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);   
		    END_IF		   
	   END_IF
	   
	60:
	   ref_Follower1.i.SelectCamTable:=TRUE; 
	   IF ref_Follower1.o.SelectCamTableDone THEN
		   ref_Follower1.i.SelectCamTable:=FALSE; 
		   FollowerSeq(ExeCute:= TRUE, Next:= 61);  
	   END_IF
	   
	61:
        MC_ReadCamTableSlaveDynamicsFollwer1.Execute:=TRUE;
	    IF MC_ReadCamTableSlaveDynamicsFollwer1.Done THEN
			lrFollow1CamDynPos:=MC_ReadCamTableSlaveDynamicsFollwer1.SlavePosition
            +TRUNC(ref_master.o.ActPos/2800)*3300;
			lrFollow1LatchPos:=ref_Follower1.o.ActPos;
			MC_ReadCamTableSlaveDynamicsFollwer1.Execute:=FALSE;
			FollowerSeq(ExeCute:= TRUE, Next:= 62); 
		END_IF  
	62:
		ref_Follower2.i.SelectCamTable:=TRUE; 
		IF ref_Follower2.o.SelectCamTableDone THEN
		   ref_Follower2.i.SelectCamTable:=FALSE; 
		   FollowerSeq(ExeCute:= TRUE, Next:= 63);  
		END_IF
	63:
        MC_ReadCamTableSlaveDynamicsFollwer2.Execute:=TRUE;
	    IF MC_ReadCamTableSlaveDynamicsFollwer2.Done THEN
			lrFollow2CamDynPos:=MC_ReadCamTableSlaveDynamicsFollwer2.SlavePosition
            +TRUNC(ref_master.o.ActPos/2800)*3300;
			lrFollow2LatchPos:=ref_Follower2.o.ActPos;
			MC_ReadCamTableSlaveDynamicsFollwer2.Execute:=FALSE;
			FollowerSeq(ExeCute:= TRUE, Next:= 64); 
		END_IF  
    64:
	    tonFollowerSeq(IN:= TRUE, PT:= T#1S, Q=> , ET=> );
		IF tonFollowerSeq.Q THEN
		   tonFollowerSeq(IN:= FALSE, PT:= T#1S, Q=> , ET=> );	
		   FollowerSeq(ExeCute:= TRUE, Next:= 70); 
		END_IF
		
    70:
		ref_Follower1.i.Pos:=lrFollow1CamDynPos;
        ref_Follower1.i.Vel:=60;
		ref_Follower1.i.Acc:=600;
		ref_Follower1.i.Dec:=600;
		
		ref_Follower2.i.Pos:=lrFollow2CamDynPos;
		ref_Follower2.i.Vel:=60;
		ref_Follower2.i.Acc:=600;
		ref_Follower2.i.Dec:=600;		
        FollowerSeq(ExeCute:= TRUE, Next:= 71);  
	71: 
	    tonFollowerSeq(IN:= TRUE, PT:= T#1S, Q=> , ET=> );
		IF tonFollowerSeq.Q THEN
		   tonFollowerSeq(IN:= FALSE, PT:= T#1S, Q=> , ET=> );	
		   FollowerSeq(ExeCute:= TRUE, Next:= 72); 	   
		END_IF	
	72:	
		ref_Follower2.i.MoveAbs:=TRUE;
		ref_Follower1.i.MoveAbs:=TRUE;
	    IF ref_Follower1.o.CmdDone AND ref_Follower1.o.StandStill
        AND ref_Follower2.o.CmdDone AND ref_Follower2.o.StandStill THEN
			ref_Follower1.i.MoveAbs:=FALSE;
			ref_Follower2.i.MoveAbs:=FALSE;
			FollowerSeq(ExeCute:= TRUE, Next:= 73); 
	    ELSE IF Ref_MachineStateModel.Sts_Stopping  THEN
				ref_Follower1.i.MoveAbs:=FALSE;
				ref_Follower2.i.MoveAbs:=FALSE;			   
				FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);   
			 END_IF			
		END_IF
		
			
	73:
	   ref_Follower1.i.CamIn:=TRUE;
       IF ref_Follower1.o.InSync  THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= 74);  
	   END_IF 
    74: 
	   ref_Follower2.i.CamIn:=TRUE;
       IF ref_Follower2.o.InSync  THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= 80);  
	   END_IF 
 
    80:
		IF glb_AllAxisHomed_UN01 THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 90);
		   ELSE 
		   FollowerSeq(ExeCute:= TRUE, Next:= 81);
		END_IF
	81:		
		IF ref_Master.o.Homed AND ref_Master.o.PowerOn THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 82);//轴组耦合
		END_IF
    82:	 			  
		lrPosCnt:=1;
		ref_Master.i.Pos:=200;
		ref_Master.i.Acc:=100;
		ref_Master.i.Dec:=100;
		glb_BinBelt4_5.bin[1].arDrop[1]:=TRUE;
		glb_BinBelt4_5.bin[1].arDrop[8]:=TRUE;
		FollowerSeq(ExeCute:= TRUE, Next:= 83);
			
	83:
		ref_Master.i.MoveAbs:=TRUE;
		IF ref_Master.o.CmdDone THEN
		   ref_Master.i.MoveAbs:=FALSE;
		   FollowerSeq(ExeCute:= TRUE,Hold:=FALSE, Next:= 90);
		END_IF				 
					  

    90:	
	   FollowerSeq(ExeCute:= TRUE, Next:= Seq_ResettingDone);
	

   	Seq_ResettingDone: 
         IF NOT Ref_MachineStateModel.Sts_Resetting THEN
			FollowerSeq(ExeCute:= TRUE, Next:= 0); 
		 END_IF	
			  
				   
						
{endregion}			  
{region Execute}
	110:   
	   FollowerSeq(ExeCute:=TRUE , Next:= 120); 
			
	120:	  
	   IF NOT glb_BinBelt4_5.bin[9].arDrop[8] 
	   AND NOT glb_BinBelt4_5.bin[1].arDrop[1] THEN
		   FollowerSeq(ExeCute:= TRUE, Next:= 121);  ;
	   END_IF
	   IF Ref_MachineStateModel.Sts_Stopping  THEN
		  FollowerSeq(ExeCute:= TRUE, Next:= Seq_Stopping);  
	   END_IF
	
    121:
		 tonDelayMotion(IN:= TRUE, PT:= T#800MS, Q=> , ET=> );
		 IF tonDelayMotion.Q THEN
            tonDelayMotion(IN:= FALSE, PT:= T#800MS, Q=> , ET=> );
			FollowerSeq(ExeCute:= TRUE, Next:= 130,hold:=FALSE); 	 
         END_IF	  
 		 			  
	130: 
        ref_Master.i.Pos:= ref_Master.i.Pos+200;
		ref_Master.i.Acc:=50;
		ref_Master.i.Dec:=100;
		ref_Master.i.Vel:=100;
        FollowerSeq(ExeCute:= TRUE,hold:=FALSE, Next:= 140);  
		
	140:  
 
		ref_Master.i.MoveAbs:=TRUE;
		IF ref_Master.o.CmdDone THEN
		   ref_Master.i.MoveAbs:=FALSE;
		   FollowerSeq(ExeCute:= TRUE,Hold:=FALSE, Next:= 150);
		END_IF

		IF  ref_Master.i.Pos - ref_Master.o.ActPos <90 AND NOT bTriggerPos  THEN
	       glb_BinBelt4_5.bin[1].arDrop[1]:=TRUE;
		   bTriggerPos:=TRUE;
	    END_IF
	  
	     
	150:           
          bTriggerPos:=FALSE;
          glb_lrMasterWrkCnt:=glb_lrMasterWrkCnt+1;
		  FOR i:=16 TO 1 BY -1 DO
			  glb_BinBelt4_5.bin[i]:= glb_BinBelt4_5.bin[i-1];
		  END_FOR
		  FOR i:=16 TO 2 BY -1 DO
			  glb_BinBelt4_5.bin[i].arDrop[1]:= FALSE;
		  END_FOR			   
	      glb_BinBelt4_5.bin[1].arDrop[8]:=TRUE;		
		  FollowerSeq(ExeCute:=TRUE ,Hold:=FALSE, Next:= 110); 	   
	   
				  
{endRegion}
	Seq_Stopping: 
       ref_Master.i.Enable:=FALSE; 
	   IF NOT ref_Master.o.PowerOn THEN
			  FollowerSeq(ExeCute:= TRUE, Next:= 901); 	   
	   END_IF
	   
    901: 
	    IF NOT ref_Follower1.o.StandStill AND NOT ref_Follower1.o.InSync THEN
			ref_Follower1.i.Stop:=TRUE;
		END_IF	
	    IF NOT ref_Follower2.o.StandStill AND NOT ref_Follower2.o.InSync THEN
			ref_Follower2.i.Stop:=TRUE;
		END_IF
	    IF ref_Follower1.o.StandStill AND ref_Follower2.o.StandStill THEN
		   ref_Follower1.i.Stop:=FALSE;
		   ref_Follower2.i.Stop:=FALSE;
           FollowerSeq(ExeCute:= TRUE, Next:= 902); 		 
		END_IF
		
    902: 	

		IF ref_Follower1.o.InGear THEN 
		   ref_Follower1.i.GearOut:=TRUE;	
		END_IF	
		IF ref_Follower2.o.InGear THEN 
		   ref_Follower2.i.GearOut:=TRUE;	
		END_IF
		   FollowerSeq(ExeCute:= TRUE, Next:= 903); 
	903:
	    IF ref_Follower1.o.InSync THEN 
		   ref_Follower1.i.CamOut:=TRUE;	
		END_IF	
		IF ref_Follower2.o.InSync THEN 
		   ref_Follower2.i.CamOut:=TRUE;	
		END_IF	
		FollowerSeq(ExeCute:= TRUE, Next:= 904); 
	904:	
	    tonFollowerSeqDelay(IN:=TRUE , PT:=T#1S , Q=> , ET=> );
		IF tonFollowerSeqDelay.Q THEN
			tonFollowerSeqDelay(IN:=FALSE , PT:=T#1S , Q=> , ET=> );
			FollowerSeq(ExeCute:= TRUE, Next:= Seq_StoppingDone); 
		END_IF
			
	Seq_StoppingDone:
	     ref_Master.i.Disable:=TRUE;
	     ref_Follower1.i.Disable:=TRUE;
		 ref_Follower2.i.Disable:=TRUE;
		 IF NOT Ref_MachineStateModel.Sts_Stopping THEN
			FollowerSeq(ExeCute:= TRUE, Next:= 0); 	
		 END_IF
END_CASE
rtMasterInRange(CLK:= (ref_Master.o.ActPos- ref_Master.i.Pos >110)	, Q=> ); 
FollowerSeq.Hold := FALSE;]]></ST>
      </Implementation>
    </Action>
    <Action Name="MainRoution" Id="{a1e8189a-98e2-4f7f-bae9-70891832d49e}">
      <Implementation>
        <ST><![CDATA[EM_No:=6;
IF Ref_Unit.EM_Selected[EM_No] THEN
	IF Ref_MachineStateModel.Sts_Holding THEN
		Ref_Unit.EM_Holding_Done[EM_No]:=TRUE;
		ELSE
		Ref_Unit.EM_Holding_Done[EM_No]:=FALSE;
	END_IF
	IF Ref_MachineStateModel.Sts_UnHolding THEN
		Ref_Unit.EM_UnHolding_Done[EM_No]:=TRUE;
		ELSE
		Ref_Unit.EM_UnHolding_Done[EM_No]:=FALSE;
	END_IF
	
	IF Ref_MachineStateModel.Sts_Suspending THEN
		Ref_Unit.EM_Suspending_Done[EM_No]:=TRUE;
		ELSE
		Ref_Unit.EM_Suspending_Done[EM_No]:=FALSE;
	END_IF
	
	IF Ref_MachineStateModel.Sts_UnSuspending THEN
		Ref_Unit.EM_UnSuspending_Done[EM_No]:=TRUE;
		ELSE
		Ref_Unit.EM_UnSuspending_Done[EM_No]:=FALSE;
	END_IF

	IF Ref_MachineStateModel.Sts_Resetting  AND FollowerSeq.Now=Seq_ResettingDone THEN
	   Ref_Unit.EM_Resetting_Done[EM_No]:=TRUE;
       ELSE   
	   Ref_Unit.EM_Resetting_Done[EM_No]:=FALSE;
	END_IF
	
	IF Ref_MachineStateModel.Sts_Clearing THEN
	   Ref_Unit.EM_Clearing_Done[EM_No]:=TRUE;
	   ELSE
	   Ref_Unit.EM_Clearing_Done[EM_No]:=FALSE;
	END_IF
	

	IF Ref_MachineStateModel.Sts_Starting THEN
	   Ref_Unit.EM_Starting_Done[EM_No]:=TRUE;
	   ELSE
       Ref_Unit.EM_Starting_Done[EM_No]:=FALSE;
	END_IF
	
	IF Ref_MachineStateModel.Sts_Stopping AND FollowerSeq.Now=Seq_StoppingDone  THEN
	   Ref_Unit.EM_Stopping_Done[EM_No]:=TRUE;	
       ELSE  
	   Ref_Unit.EM_Stopping_Done[EM_No]:=FALSE;	
	END_IF

	IF Ref_MachineStateModel.Sts_Aborting THEN
		FollowerSeq(ExeCute:= TRUE, Next:= 0);

		IF ref_Follower1.o.InSync THEN
			ref_Follower1.i.CamOut:=TRUE;
			ref_Follower1.i.GearOut:=TRUE;
		ELSE IF NOT ref_Follower1.o.StandStill THEN
				ref_follower1.i.Stop:=TRUE;
				END_IF		   
		END_IF
		
		IF ref_Follower2.o.InSync THEN
			ref_Follower2.i.CamOut:=TRUE;
			ref_Follower2.i.GearOut:=TRUE;
		ELSE IF NOT ref_Follower2.o.StandStill THEN
			ref_follower2.i.Stop:=TRUE;
			END_IF		   
		END_IF		   
	   
		IF tonAbortingWatchDog.Q THEN 
			Ref_Unit.EM_Aborting_Done[EM_No]:=TRUE;
			ref_Follower1.i.Disable:=TRUE;
			ref_Follower2.i.Disable:=TRUE;
        END_IF
    ELSE
		Ref_Unit.EM_Aborting_Done[EM_No]:=FALSE;
	END_IF
	
	tonAbortingWatchDog(IN:= Ref_MachineStateModel.Sts_Aborting, PT:=T#3S , Q=> , ET=> );
	CM03_Follower();
ELSE
		FollowerSeq(ExeCute:= TRUE, Next:= 0);		
END_IF]]></ST>
      </Implementation>
    </Action>
